<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- ==================== HYDROGEN STORAGE TANKS ==================== -->

	<!-- Tiny Hydrogen Tank (1x1) -->
	<ThingDef ParentName="BuildingBase">
		<defName>HydrogenTankTiny</defName>
		<label>tiny hydrogen tank</label>
		<description>A small pressurized tank for storing hydrogen gas. Connects to the hydrogen pipe network and acts as a buffer between production and consumption. No power required. Storage: 100 H2.</description>
		<thingClass>DubsElectrolysis.Building_GasStorageTank</thingClass>
		<designatorDropdown>HydrogenTank</designatorDropdown>
		<graphicData>
			<texPath>Things/Building/Storage/HydrogenTankTiny</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>40</pathCost>
		<blockWind>false</blockWind>
		<fillPercent>0.35</fillPercent>
		<canOverlapZones>false</canOverlapZones>
		<statBases>
			<WorkToBuild>800</WorkToBuild>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Mass>15</Mass>
		</statBases>
		<tickerType>Normal</tickerType>
		<size>(1,1)</size>
		<costList>
			<Steel>25</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<building>
			<canPlaceOverWall>false</canPlaceOverWall>
		</building>
		<comps>
			<li Class="DubsElectrolysis.CompProperties_GasPipe">
				<gasType>Hydrogen</gasType>
				<storageCapacity>100</storageCapacity>
			</li>
		</comps>
		<designationCategory>Misc</designationCategory>
		<researchPrerequisites>
			<li>Electrolysis</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
	</ThingDef>

